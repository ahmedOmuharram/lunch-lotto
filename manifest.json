{
    "manifest_version": 3,
    "name": "Eye Relaxation Reminder",
    "version": "1.0",
    "description": "A Chrome extension to remind users to relax their eyes with short exercises.",
    "permissions": ["notifications", "alarms", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  